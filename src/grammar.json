{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/grammar.schema.json",
  "name": "supercollider",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_item"
      }
    },
    "_item": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "line_comment"
        },
        {
          "type": "SYMBOL",
          "name": "block_comment"
        },
        {
          "type": "SYMBOL",
          "name": "symbol"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "SYMBOL",
          "name": "string"
        },
        {
          "type": "SYMBOL",
          "name": "keyword"
        },
        {
          "type": "SYMBOL",
          "name": "ugen"
        },
        {
          "type": "SYMBOL",
          "name": "pattern_class"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "PATTERN",
          "value": "."
        }
      ]
    },
    "line_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "//"
        },
        {
          "type": "PATTERN",
          "value": ".*"
        }
      ]
    },
    "block_comment": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "/*"
        },
        {
          "type": "PATTERN",
          "value": "[^*]*\\*+([^/*][^*]*\\*+)*"
        },
        {
          "type": "STRING",
          "value": "/"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+(\\.\\d+)?"
    },
    "string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\""
        },
        {
          "type": "PATTERN",
          "value": "[^\"]*"
        },
        {
          "type": "STRING",
          "value": "\""
        }
      ]
    },
    "symbol": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "\\"
        },
        {
          "type": "PATTERN",
          "value": "[a-zA-Z_][a-zA-Z0-9_]*"
        }
      ]
    },
    "keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "var"
        },
        {
          "type": "STRING",
          "value": "arg"
        },
        {
          "type": "STRING",
          "value": "if"
        },
        {
          "type": "STRING",
          "value": "else"
        },
        {
          "type": "STRING",
          "value": "while"
        },
        {
          "type": "STRING",
          "value": "for"
        },
        {
          "type": "STRING",
          "value": "do"
        },
        {
          "type": "STRING",
          "value": "true"
        },
        {
          "type": "STRING",
          "value": "false"
        },
        {
          "type": "STRING",
          "value": "nil"
        },
        {
          "type": "STRING",
          "value": "this"
        },
        {
          "type": "STRING",
          "value": "super"
        }
      ]
    },
    "ugen": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "SynthDef"
        },
        {
          "type": "STRING",
          "value": "SinOsc"
        },
        {
          "type": "STRING",
          "value": "Saw"
        },
        {
          "type": "STRING",
          "value": "Pulse"
        },
        {
          "type": "STRING",
          "value": "WhiteNoise"
        },
        {
          "type": "STRING",
          "value": "PinkNoise"
        },
        {
          "type": "STRING",
          "value": "EnvGen"
        },
        {
          "type": "STRING",
          "value": "Env"
        },
        {
          "type": "STRING",
          "value": "Out"
        },
        {
          "type": "STRING",
          "value": "In"
        },
        {
          "type": "STRING",
          "value": "Mix"
        },
        {
          "type": "STRING",
          "value": "Pan2"
        },
        {
          "type": "STRING",
          "value": "LPF"
        },
        {
          "type": "STRING",
          "value": "HPF"
        },
        {
          "type": "STRING",
          "value": "BPF"
        },
        {
          "type": "STRING",
          "value": "Delay1"
        },
        {
          "type": "STRING",
          "value": "FreeVerb"
        }
      ]
    },
    "pattern_class": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "Pbind"
        },
        {
          "type": "STRING",
          "value": "Pseq"
        },
        {
          "type": "STRING",
          "value": "Prand"
        },
        {
          "type": "STRING",
          "value": "Pdef"
        },
        {
          "type": "STRING",
          "value": "Routine"
        },
        {
          "type": "STRING",
          "value": "Task"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[a-zA-Z_][a-zA-Z0-9_]*"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "line_comment"
    },
    {
      "type": "SYMBOL",
      "name": "block_comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": [],
  "reserved": {}
}